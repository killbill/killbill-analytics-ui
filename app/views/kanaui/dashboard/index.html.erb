                            </div>
                          </div>
                          <% ((@report['variables'] || {})['fields'] || []).each do |definition| %>
                            <div class="form-group">
                              <div class="col-md-7">
                                <%= definition['name'].titleize %>:
                                <% # TODO Not fully implemented server side %>
                                <% if definition['dataType'] == 'date' %>
